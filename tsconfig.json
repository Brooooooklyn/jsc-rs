{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "importHelpers": false,
    "noEmitHelpers": false,
    "skipLibCheck": true,
    "allowJs": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "jsc",
    "jsc-sys",
    "icu",
    "WebKit",
    "tyr",
    ".yarn",
    "target"
  ]
}
